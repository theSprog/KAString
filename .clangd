CompileFlags:
  Add:
    - -std=c++11
    - -DDBG_MACRO
    - -Wall
    - -Wextra
    - -Weffc++
    - -Werror=uninitialized
    - -Werror=return-type
    - -Wconversion
    - -Wsign-compare
    - -Werror=unused-result
    - -Werror=suggest-override
    - -Wzero-as-null-pointer-constant
    - -Wmissing-declarations
    - -Wold-style-cast
    - -Werror=vla
    - -Wnon-virtual-dtor
    - -Wreturn-local-addr
    - -Wno-sign-conversion
